import java.util.*;
public class Main
{
 public static void main(String []args)
 {
  Scanner ob=new Scanner(System.in);
  int a,b,c;
  System.out.println("Enter first number");
  a=ob.nextInt();                                  //scanning first number
  System.out.println("Enter second number");
  b=ob.nextInt();                                 //scanning second number
  System.out.print("sumof two numbers is"+" ");
  c=a+b;                                          //adding two numbers and storing in c
  System.out.print(c);                            //Displaying c
 }
} 
